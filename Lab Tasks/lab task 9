#include <iostream>
#include <string>
#include <limits>
using namespace std;

struct TimeCapsule {
    int year;
    string eventName;
    TimeCapsule* left;
    TimeCapsule* right;
    TimeCapsule(int y, string n) : year(y), eventName(n), left(nullptr), right(nullptr) {}
};

class TimeTree {
private:
    TimeCapsule* root;

    TimeCapsule* insertHelper(TimeCapsule* node, int year, string name, bool &inserted) {
        if (!node) {
            inserted = true;
            return new TimeCapsule(year, name);
        }
        if (year < node->year)
            node->left = insertHelper(node->left, year, name, inserted);
        else if (year > node->year)
            node->right = insertHelper(node->right, year, name, inserted);
        return node;
    }

    TimeCapsule* findMin(TimeCapsule* node) {
        while (node && node->left) node = node->left;
        return node;
    }

    TimeCapsule* deleteHelper(TimeCapsule* node, int year, bool &deleted) {
        if (!node) return nullptr;
        if (year < node->year)
            node->left = deleteHelper(node->left, year, deleted);
        else if (year > node->year)
            node->right = deleteHelper(node->right, year, deleted);
        else {
            deleted = true;
            if (!node->left && !node->right) {
                delete node;
                return nullptr;
            } else if (!node->left) {
                TimeCapsule* temp = node->right;
                delete node;
                return temp;
            } else if (!node->right) {
                TimeCapsule* temp = node->left;
                delete node;
                return temp;
            } else {
                TimeCapsule* temp = findMin(node->right);
                node->year = temp->year;
                node->eventName = temp->eventName;
                node->right = deleteHelper(node->right, temp->year, deleted);
            }
        }
        return node;
    }

    TimeCapsule* searchHelper(TimeCapsule* node, int year) {
        if (!node) return nullptr;
        if (year == node->year) return node;
        if (year < node->year) return searchHelper(node->left, year);
        return searchHelper(node->right, year);
    }

    void reportHelper(TimeCapsule* node) {
        if (!node) return;
        reportHelper(node->left);
        cout << node->year << ": " << node->eventName << endl;
        reportHelper(node->right);
    }

    void destroyTree(TimeCapsule* node) {
        if (node) {
            destroyTree(node->left);
            destroyTree(node->right);
            delete node;
        }
    }

public:
    TimeTree() { root = nullptr; }
    ~TimeTree() { destroyTree(root); }

    void inject(int year, string name) {
        cout << "> System: Injecting " << year << "... ";
        bool inserted = false;
        root = insertHelper(root, year, name, inserted);
        if (inserted) cout << "Timeline stable." << endl;
        else cout << "Year already exists. Ignored." << endl;
    }

    void resolveParadox(int year) {
        cout << "> Alert: Paradox detected at " << year << "!" << endl;
        bool deleted = false;
        root = deleteHelper(root, year, deleted);
        if (deleted) cout << "> System: Year " << year << " removed. Timeline stabilized." << endl;
        else cout << "> System: Year not found. No paradox resolved." << endl;
    }

    void search(int year) {
        cout << "> Query: Searching for " << year << "..." << endl;
        TimeCapsule* result = searchHelper(root, year);
        if (result)
            cout << "> Result: Event Found! [" << result->year << ": " << result->eventName << "]" << endl;
        else
            cout << "> Result: Year " << year << " not found in current timeline." << endl;
    }

    void report() {
        cout << "> COMMAND: CHRONOLOGICAL REPORT" << endl;
        cout << "-------------------------------" << endl;
        reportHelper(root);
        cout << "-------------------------------" << endl;
    }
};

int main() {
    TimeTree chronos;
    string command;
    int year;
    string eventName;

    cout << "--- CHRONOS TIME STABILIZER V1.0 ---" << endl;
    cout << "Waiting for Time Stream Inputs..." << endl << endl;

    while (cin >> command) {
        if (command == "INJECT") {
            cin >> year;
            getline(cin >> ws, eventName);
            chronos.inject(year, eventName);
        }
        else if (command == "PARADOX") {
            cin >> year;
            chronos.resolveParadox(year);
        }
        else if (command == "SEARCH") {
            cin >> year;
            chronos.search(year);
        }
        else if (command == "REPORT") {
            chronos.report();
        }
        else if (command == "EXIT") {
            break;
        }
        else {
            cout << "> Unknown Command." << endl;
            cin.clear();
            cin.ignore(numeric_limits<streamsize>::max(), '\n');
        }
        cout << endl;
    }

    return 0;
}
